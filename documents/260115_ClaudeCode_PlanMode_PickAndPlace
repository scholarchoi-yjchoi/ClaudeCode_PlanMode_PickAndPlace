        â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€  â† ë°”ë‹¥

     3.3 êµ¬í˜„ ì½”ë“œ

     def create_pedestal(world: World, position: np.ndarray):
         """4-column pedestal that minimizes RMPFlow interference."""

         column_radius = 0.005  # 1cm diameter
         column_height = 0.25   # Height to support object at z=0.27
         column_spacing = 0.04  # 8cm total footprint

         # 4 columns at corners
         column_offsets = [
             np.array([column_spacing, column_spacing, column_height/2]),
             np.array([column_spacing, -column_spacing, column_height/2]),
             np.array([-column_spacing, column_spacing, column_height/2]),
             np.array([-column_spacing, -column_spacing, column_height/2]),
         ]

         for i, offset in enumerate(column_offsets):
             # Use thin cylinder or small cuboid
             FixedCuboid(
                 prim_path=f"/World/Pedestal/Column{i}",
                 name=f"pedestal_column_{i}",
                 position=position + offset,
                 scale=np.array([column_radius*2, column_radius*2, column_height]),
                 size=1.0,
                 color=np.array([0.5, 0.5, 0.5]),
             )

         # Thin top plate
         FixedCuboid(
             prim_path="/World/Pedestal/TopPlate",
             name="pedestal_top",
             position=position + np.array([0, 0, column_height]),
             scale=np.array([0.08, 0.08, 0.01]),  # 8cm x 8cm, 1cm thick
             size=1.0,
             color=np.array([0.6, 0.6, 0.6]),
         )

