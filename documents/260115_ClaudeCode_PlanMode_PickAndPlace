     Step 1: ìƒˆ íŒŒì¼ ìƒì„± (franka_task_pickplace.py)
     from isaacsim.robot.manipulators.examples.franka import PickPlace

     class CustomPickPlaceTask(PickPlace):
         def __init__(self, name="pick_place_task"):
             super().__init__(name=name)
             self._pick_target = None
             self._place_target = None

         def set_up_scene(self, scene):
             super().set_up_scene(scene)
             # í…Œì´ë¸”, ì»¨í…Œì´ë„ˆ, íë¸Œ ì¶”ê°€
             self._cube = DynamicCuboid(
                 prim_path="/World/PickCube",
                 position=np.array([0.3, 0.3, 0.265]),
                 scale=np.array([0.05, 0.05, 0.05]),
             )
             scene.add(self._cube)

         def post_reset(self):
             super().post_reset()
             # Physics settle í›„ ì‹¤ì œ ìœ„ì¹˜ íšë“
             self._pick_target = self._cube.get_world_pose()[0]

